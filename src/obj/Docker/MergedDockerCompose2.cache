services:
  thefipster.dysonsphere.seed.api:
    build:
      context: C:\Users\felix\github\dyson-sphere-program\src
      dockerfile: api/TheFipster.DysonSphere.Seed.Api/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: TheFipster.DysonSphere.Seed.Api
      target: base
    container_name: TheFipster.DysonSphere.Seed.Api
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: thefipsterdysonsphereseedapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/TheFipster.DysonSphere.Seed.Api.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - C:\Users\felix\github\dyson-sphere-program\src\api\TheFipster.DysonSphere.Seed.Api:/app:rw
    - C:\Users\felix\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\felix\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\felix\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\felix\.nuget\packages:/root/.nuget/packages:ro
    - C:\Users\felix\github\dyson-sphere-program\src:/src:rw
version: '3.4'